void prime(int i)
{
	int j = 1;
	int flag = 1;
	if (i == 1||i ==2)
		printf("这个数是素数");
	else if  (flag==1)
	{
		for (j = 2; j < i; j++)
		{
			if (i % j == 0)
			{
				flag = 0;
				break;
			}

		}
		if (flag == 0)
			printf("这个数不是素数");
		else if (flag == 1)
			printf("这个数是素数");
	}
	

}
int main()
{
	int a = 77;
	prime(a);
	return 0;
}
