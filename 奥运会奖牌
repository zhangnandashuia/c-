#include<stdio.h>
int main()
{
	int n = 0;
	int s=0, d=0, f = 0;
	int gold = 0;
	int silver = 0;
	int copper = 0;
	scanf_s("%d", &n);
	for (int i = 0; i < n; i++)
	{
		scanf_s("%d %d %d", &gold, &silver, &copper);
		s += gold;
		d += silver;
		f += copper;
		
	}
	printf("%d %d %d %d", s, d, f,s+d+f);
	return 0;
}
