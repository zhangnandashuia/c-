#include<stdio.h>
int main()
{
	int num = 0;
	int show1 = 0, show2 = 0, show3 = 0;
	int n;
	scanf_s("%d", &num);
	for (int i = 0; i < num; i++)
	{
		scanf_s("%d", &n);
		if (n == 1)
			show1 += 1;
		else if (n == 5)
			show2 += 1;
		else if (n == 10)
			show3 += 1;
	}
	printf("%d\n%d\n%d", show1, show2, show3);
}
